{
  "rules": {
    "camelcase": 0,
    "quotes": 0,
    "no-unused-vars": 0,
    "global-strict": 0,
    "strict": 0,
    "no-trailing-spaces": 0,
    "no-console": 0,
    "eol-last": 0,
    "no-alert": 2,
    "no-mixed-spaces-and-tabs": 0,
    "dot-notation": 0,
    "new-cap": 0,
    "semi": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "no-multi-spaces": 0,
    "key-spacing":0
  },
  "env": {
    "browser": true,
    "node": true,
    "amd" : true,
    "mocha": true,
    "jasmine":true,
    "jquery": true
  },
  "globals": {
    "require" : false
  }
}